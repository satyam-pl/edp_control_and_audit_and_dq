  COMPLETENESS_FLAG STRING,
  CONFORMITY_FLAG STRING,
  CONSISTENCY_FLAG STRING,
  INTEGRITY_FLAG STRING,
  TIMELINESS_FLAG STRING,
  UNIQUENESS_FLAG STRING)
USING sqlserver
