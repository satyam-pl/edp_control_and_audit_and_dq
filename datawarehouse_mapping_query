CREATE TABLE edp_edproddb_srv01.edp_meta.datawarehouse_mapping (
  SUBJECT_AREA VARCHAR(1000),
  LOGICAL_TABLE_NAME VARCHAR(1000),
  ELEMENT_NAME VARCHAR(1000),
  EDW_FIELD VARCHAR(500),
  EDW_TABLE_NAME VARCHAR(1000),
  EDW_COLUMN_NAME VARCHAR(1000),
  SOURCE_SCHEMA VARCHAR(500),
  SOURCE_TABLE_NAME VARCHAR(1000),
  SOURCE_COLUMN_NAME VARCHAR(1000),
  SOURCE_BUSINESS_RULE VARCHAR(2147483647))
USING sqlserver
