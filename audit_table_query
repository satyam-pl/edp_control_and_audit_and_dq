CREATE TABLE edp_edproddb_srv01.edp_meta.audit_table (
  AUDIT_KEY BIGINT,
  SOURCE_OBJECT_KEY BIGINT,
  BATCH_ID VARCHAR(100),
  ETL_RUN_DATE DATE,
  LAYER_NAME VARCHAR(50),
  PIPELINE_RUNID VARCHAR(100),
  PIPELINE_TRIGGER_NAME VARCHAR(100),
  TRIGGERED_BY_PIPELINE VARCHAR(100),
  SOURCE_TABLE VARCHAR(200),
  TARGET_TABLE VARCHAR(100),
  PIPELINE_NAME VARCHAR(400),
  LOAD_FROM_DATETIME TIMESTAMP,
  LOAD_TO_DATETIME TIMESTAMP,
  PIPELINE_START_TIME TIMESTAMP,
  PIPELINE_END_TIME TIMESTAMP,
  TASK_DURATION_SECONDS VARCHAR(50),
  TARGET_ROW_COUNT VARCHAR(50),
  ROWS_EXTRACTED VARCHAR(50),
  ROWS_INSERTED VARCHAR(50),
  ROWS_UPDATED VARCHAR(50),
  ROWS_DELETED VARCHAR(50),
  TOTAL_ROW_COUNT VARCHAR(50),
  FILE_SIZE_KB VARCHAR(50),
  LOAD_STATUS VARCHAR(50),
  ERROR_MESSAGE VARCHAR(1000),
  PARTITION_KEY_VALUE VARCHAR(200),
  delta_max_value BIGINT)
USING sqlserver
